{
  "env": {
      "es2021": true,
      "node": true,
      "browser": true,
      "jest": true
  },
  "extends": [
      "airbnb",
      "airbnb/hooks"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "html"
  ],
  "rules": {
      "react/jsx-props-no-spreading": "off",
      "react/require-default-props": "off",
      "react/forbid-prop-types": "off",
      "import/prefer-default-export": "off"
  }
}